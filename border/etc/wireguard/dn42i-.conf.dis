[Interface]
# 己方 WireGuard 私钥
PrivateKey = 
# 己方端口号
ListenPort = 
Table = off
# 己方 Link-local IPv6（例如 fe80::1234）
PostUp = ip addr add fe80::XXXX/64 dev %i
# 己方 DN42 IPv6 地址（例如 fd12:3456:7890::1）
PostUp = ip addr add fda5:adba:5953::XXXX/128 dev %i
# 第一个是己方的 DN42 内的 IP，第二个是对方的
PostUp = ip addr add 172.21.123.XX peer 172.21.123.XX dev %i
PostUp = sysctl -w net.ipv6.conf.%i.autoconf=0

[Peer]
# 对方的公钥
PublicKey = 
# 对方服务器地址和端口号，端口号一般为你的 ASN 的后五位
Endpoint = XX:XXXXX
AllowedIPs = 10.0.0.0/8, 172.20.0.0/14, 172.31.0.0/16, fd00::/8, fe80::/64, ff02::/16
